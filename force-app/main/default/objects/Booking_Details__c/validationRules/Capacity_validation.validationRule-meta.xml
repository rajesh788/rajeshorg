<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Capacity_validation</fullName>
    <active>true</active>
    <errorConditionFormula>Total_Members__c &gt;   VALUE(TEXT(Room__r.Capacity__c ))</errorConditionFormula>
    <errorDisplayField>Total_Members__c</errorDisplayField>
    <errorMessage>Total Members should be less than or equal to Room Capacity.</errorMessage>
</ValidationRule>
