<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Available_Rooms_validation_rule</fullName>
    <active>true</active>
    <errorConditionFormula>Available_Rooms__c  &gt;  10  || (ISBLANK(Available_Rooms__c) )</errorConditionFormula>
    <errorMessage>Maximum room can be 10.</errorMessage>
</ValidationRule>
